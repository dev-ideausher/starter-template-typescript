{
    "name": "starter-template-typescript",
    "version": "1.0.0",
    "description": "This is a TypeScript starter template for building backend applications with Express.",
    "main": "dist/server.js",
    "type": "module",
    "scripts": {
        "dev": "concurrently -k -n TSC,ALIAS,NODE -c yellow,cyan,green \"tsc -w\" \"tsc-alias -w\" \"nodemon dist/server.js\"",
        "build": "tsc && tsc-alias",
        "start": "node dist/server.js"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.879.0",
        "@types/compression": "^1.8.1",
        "@types/morgan": "^1.9.10",
        "apple-signin-auth": "^1.7.5",
        "bcryptjs": "^2.4.3",
        "cloudinary": "^2.7.0",
        "compression": "^1.8.1",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^6.11.2",
        "firebase-admin": "^11.10.1",
        "google-auth-library": "^9.0.0",
        "helmet": "^7.2.0",
        "hpp": "^0.2.3",
        "http-status": "^2.1.0",
        "joi": "^17.9.2",
        "jsonwebtoken": "^9.0.2",
        "module-alias": "^2.2.3",
        "mongoose": "^7.5.0",
        "morgan": "^1.10.1",
        "multer": "^2.0.2",
        "nodemailer": "^6.9.4",
        "winston": "^3.17.0",
        "xss-clean": "^0.1.4"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.17",
        "@types/hpp": "^0.2.6",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/multer": "^2.0.0",
        "@types/node": "^20.5.0",
        "@types/nodemailer": "^6.4.9",
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "@typescript-eslint/parser": "^6.4.0",
        "concurrently": "^9.2.1",
        "nodemon": "^3.1.10",
        "rimraf": "^6.0.1",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.8.16",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.6"
    },
    "_moduleAliases": {
        "@": "dist"
    },
    "keywords": [
        "express",
        "typescript",
        "node"
