{
    "name": "starter-template-typescript",
    "version": "1.0.0",
    "description": "This is a TypeScript starter template for building backend applications with Express.",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "dev": "concurrently -k -n TSC,ALIAS,NODE -c yellow,cyan,green \"tsc -w\" \"tsc-alias -w\" \"nodemon dist/index.js\"",
        "build": "tsc && tsc-alias",
        "start": "node dist/index.js"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.890.0",
        "@types/compression": "^1.8.1",
        "@types/morgan": "^1.9.10",
        "apple-signin-auth": "^2.0.0",
        "bcryptjs": "^3.0.2",
        "cloudinary": "^2.7.0",
        "compression": "^1.8.1",
        "cors": "^2.8.5",
        "dotenv": "^17.2.2",
        "express": "^5.1.0",
        "express-rate-limit": "^8.1.0",
        "firebase-admin": "^13.5.0",
        "google-auth-library": "^10.3.0",
        "helmet": "^8.1.0",
        "hpp": "^0.2.3",
        "http-status": "^2.1.0",
        "joi": "^18.0.1",
        "jsonwebtoken": "^9.0.2",
        "module-alias": "^2.2.3",
        "mongoose": "^8.18.1",
        "morgan": "^1.10.1",
        "multer": "^2.0.2",
        "sanitize-html": "^2.17.0",
        "winston": "^3.17.0",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/hpp": "^0.2.6",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/multer": "^2.0.0",
        "@types/node": "^24.5.1",
        "@typescript-eslint/eslint-plugin": "^8.44.0",
        "@typescript-eslint/parser": "^8.44.0",
        "concurrently": "^9.2.1",
        "eslint": "^9.35.0",
        "nodemon": "^3.1.10",
        "rimraf": "^6.0.1",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.8.16",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.6"
    },
    "overrides": {
        "glob": "^10.4.2",
        "rimraf": "^6.0.1",
        "inflight": "npm:lru-cache",
        "q": "npm:promise-polyfill"
    },
    "_moduleAliases": {
        "@": "dist"
    },
    "keywords": [
        "express",
        "typescript",
        "node"
    ],
    "author": "Abhay Pratap Yadav",
    "license": "MIT"
}
